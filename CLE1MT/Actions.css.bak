body {
	background-image:url(bg.jpg);
	font-family: arial;
}
.container .card{
	width:100px
	position: relative;
	height: 200px;
	background: #f6f8f6;
	margin: 15px 5px;
	padding: 20px 15px;
	border-radius: 30px;
}
.container1 .card1{
	width:100px
	position: relative;
	height: 450px;
	background: #f6f8f6;
	margin: 15px 5px;
	padding: 20px 15px;
	border-radius: 30px;
}


.title {
	font-size: 30px;
	margin-left: 80px;
	font-family: arial;
	text-align: center;
}
ul{
	float: middle;
	list-style-type: none;
}

ul li{
	display: inline-block;
}

ul li a{
	text-decoration:none;
	color: #000023;
	padding: 5px 20px;
	border: 1px solid transparent;
	transition: 0.6s ease;
}
ul li a:hover{
	background-color: #fff;
	color: #000023;
}

ul li.Poverty.html a{
	background-color: #fff;
	color: #000023;
}
	
a {
	color: #000000;
	text-decoration; underline;
}
	
a:hover {
	color: #000000; 
	text-decoration: none;
}

